#include<stdio.h>
struct fraction
{
        int numerator;
        int denominator;
};
int elememts()
{
        int n;
        printf("enter the number of fraction to be added:");
        scanf("%d",&n);
}
void input_fraction(int n,int a[n],int b[n])
{
        int i;
        printf("enter the numerators of fractions to be added:");
        for(i=0;i<n;i++)
        {
                scanf("%d",&a[i]);
        }
        printf("enter the denominators of the fractions to be added:");
        for(i=0;i<n;i++)
        {
                scanf("%d",&b[i]);
        }
}

