#include<stdio.h>
int input()
{
        int n;
        printf(" how many numbers u want add:");
        scanf("%d",&n);
        int i,add[n];
        for(i=0;i<n;i++)
        {
                scanf("%d",&add[i]);
        }
}
int compute(int n)
{
        int i,add[n],s=0;
        for(i=0;i<n;i++)
        {
                s=s+add[i];
        }
}
void output(int s)
{
        printf("%d is the sum",s);
}
int main()
{
        int n,s;
        n=input();
        s=compute(n);
        output(s);
}
